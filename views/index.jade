extends layout
block content



    section
        h1= title
        p #{message}
        p #{list[0]._fields[0]}
        form(action='/', method='get')
            +listData(list)
    div.movies
        if(index)
            +movies(index, list)


    aside
        .sider
            each obj in list
                p.lister
                    span.actor #{obj._fields[0]}